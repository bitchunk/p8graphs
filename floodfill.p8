function floodfill(x,y,p)
--  if (pget(x,y)<=1) return
  if (pget(x,y)==p) return
  pset(x,y,p)
  floodfill(x-1,y,p)
  floodfill(x+1,y,p)
  floodfill(x,y-1,p)
  floodfill(x,y+1,p)
end

function floodfill(x,y,p)
	if pget(x,y)~=p then
		pset(x,y,p)
--		return floodfill(x-1,y,p,x+1,y,p,x,y-1,p,x,y+1,p)
		return floodfill(x-1,y,floodfill(x+1,y,floodfill(x,y-1,floodfill(x,y+1,p))))
	end
--	return floodfill(...)
	return p
end

