function _3_bird()
local incr=1/32
local points,angs=split("10,10,7,6,30,55,24,7,20,20,20,7,24,55,30,6,7,10"),
split("16,18,18,21,21,23,24,30,30,0,2,2,8,9,11,11,14,14")
cls()

if #parts<1 then
	for i=1,#points do
		local p={}
		p.cx,p.cy=64,64
		p.ang,p.ang1=
		angs[i]*incr,angs[i]*incr
		p.o=points[i]
		add(parts,p)
	end
end

	line() --fix line init
	for i=0,#points do --fix loop count +1
		local p=parts[i%#points+1] --fix table references
		p.x,p.y=sin(p.ang1)*p.o+p.cx,cos(p.ang)*p.o+p.cy				
			line(p.x,p.y,9)
	end
end
